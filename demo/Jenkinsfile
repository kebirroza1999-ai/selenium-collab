pipeline{
agent{
    docker{
      image 'maven:latest'
      args '--entrypoint='
    }
    
}
stages{
    stage("dependance"){
        steps{
            sh 'mvn --version'
            sh 'mvn clean compile'
            
        }
    }
    stage("execution"){
        steps{
            sh "mvn test -Durl='https://www.saucedemo.com' --Dgroups='@regression'";
        }
    }
}


}
